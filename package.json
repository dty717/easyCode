{
	"name": "myeasycode",
	"displayName": "myEasyCode",
	"description": "easy for code",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:easycode.autoPaste",
		"onCommand:easycode.autoCopy",
		"onView:nodeDependencies"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "easycode.autoPaste",
				"title": "Hello World",
				"tooltip": "abc"
			},
			{
				"command": "easycode.autoCopy",
				"title": "First Test"
			}
		],
		"keybindings":[
			{
				"command": "easycode.autoCopy",
				"key": "ctrl+alt+c"
			},
			{
				"command": "easycode.autoPaste",
				"key": "ctrl+alt+v"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "easycode.autoCopy",
					"when": "editorLangId == markdown"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "nodeDependencies",
					"name": "Node Dependencies"
				}
			]
		}
	},
	"publisher": "dty717",
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dty717/easyCode"
	}
}